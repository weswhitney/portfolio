

      <div class="row">
  {{#each project in model}}
        <a {{bind-attr href=project.url}}>
          <div class="col-md-4">
            <div class="thumbnail">
              <img {{bind-attr src=project.image}}/>
              <div class="caption">
                <h3>{{project.name}}</h3>
                <p>{{project.description}}</p>
              </div>
            </div>
          </div>
        </a>
  {{/each}}
      </div>
