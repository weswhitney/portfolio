    <div class="page-header">
      <h2 class="text-center">Projects</h2>
    </div>
    <div class="container">
      {{#each project in model}}
            <br>
            <br>
            {{#if project.url}}
              <a {{bind-attr href=project.url}}> <strong><h3>{{project.name}}</h3></strong></a>
            {{else}}
              <strong><h3>{{project.name}}</h3></strong>
            {{/if}}
            {{project.description}}
            <img class="project-screenshot"{{bind-attr src=project.image}}/>
        <hr>
      {{/each}}
    </div>
